{
  "name": "machinjiri/installer",
  "description": "Installer for Machinjiri PHP Framework",
  "type": "composer-plugin",
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Precious Lyson",
      "email": "precious.lyson@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Mlangeni Group",
      "email": "mlangenigroupqm@gmail.com",
      "role": "Owner"
    }
  ],
  "require": {
    "php": ">=8.0",
    "composer-plugin-api": "^2.0",
    "symfony/console": "^6.0 || ^7.0",
    "symfony/process": "^6.0 || ^7.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "composer/composer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Preciouslyson\\MachinjiriInstaller\\": "src/"
    }
  },
  "extra": {
    "class": "Preciouslyson\\MachinjiriInstaller\\Plugin"
  },
  "bin": ["bin/machinjiri"],
  "scripts": {
    "post-autoload-dump": [
      "Preciouslyson\\MachinjiriInstaller\\ComposerScripts::postAutoloadDump"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}